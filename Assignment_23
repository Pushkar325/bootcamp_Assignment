// 8. Write a C++ program to swap values of two int variables without using third variable

/*
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int first,second;
    cout<<"Enter the first number"<<endl;
    cin>>first;
    cout<<"Enter the second number"<<endl;
    cin>>second;
    cout<<"Before swaping :"<<first<<" "<<second<<endl;
    swap(first,second);  // inbuilt function use kar raha hu mai
    cout<<"After swaping : "<<first<<" "<<second<<endl;

}
*/

// 9. Write a C++ program to find the maximum of two numbers.

