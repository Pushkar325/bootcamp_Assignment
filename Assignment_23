// 4. Write a C++ program to calculate the area of a circle

/*
#include<bits/stdc++.h>
using namespace std;
#define PI 3.14

int main()
{
    float r;
    cout<<"Enter the radius"<<endl;
    cin>>r;
    float area=PI*(r*r);
    cout<<"Area of circle : "<<area<<endl;
}
*/

// 5. Write a C++ program to calculate the volume of a cuboid.

/*
